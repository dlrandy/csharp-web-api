{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "MyBGList": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      //"LogLevel": {
      //  "MyBGList": "Warning"
      //},
      "FormatterOptions": {
        "TimestampFormat": "HH:mm:ss",
        "UseUtcTimestamp": true,
        "IncludeScopes": true
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "MyBGList": "Information"
      }
    },
    "Enrich": [
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "*",
  "UseDeveloperExceptionPage": false,
  "UseSwagger": false
}

